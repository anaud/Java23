package sam1;

import java.util.Scanner;
public class Hello2030 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("두 정수를 입력하세요>> ");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = (a + b);
		System.out.println (a+"+"+b +"은 " + c);
	}

}
